-- 免费开源支持主播好吧
local GameScripts = {
    ["3623096087"] = "https://raw.githubusercontent.com/XxwanhexxX/321/refs/heads/main/muscle%20legends",
    ["3101667897"] = "https://raw.githubusercontent.com/XxwanhexxX/321/refs/heads/main/Legends%20Of%20Speed",
    ["4639625707"] = "https://raw.githubusercontent.com/XxwanhexxX/321/refs/heads/main/war%20tycoon"
}
local UniversalScript = "https://raw.githubusercontent.com/XxwanhexxX/321/refs/heads/main/%E4%B8%BB%E7%BC%9D%E5%90%88"

local function InjectScript(url)
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)
    if success then
        loadstring(response)()
    end
end

local function CheckAndInject()
    local GameID = tostring(game.PlaceId)
    if GameScripts[GameID] then
        InjectScript(GameScripts[GameID])
    else
        InjectScript(UniversalScript)
    end
end

CheckAndInject()
